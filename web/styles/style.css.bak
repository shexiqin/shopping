/* CSS Document */

body {
	font-size:12px;
	background:url(../images/01.gif) repeat-x top #bbd5e4;
}
p {
	line-height:25px;
	text-indent: 2em;
}
ul, li {
	list-style:none;
}
a {
	color:#1E50A2;
	text-decoration:none;
}
a:Hover {
	color:#FF6600;
	text-decoration:none;
}
.ali_right {
	text-align:right;
}
.ali_center {
	text-align:center;
	text-indent:0;
}
.flo_left {
	float:left;
}
.flo_right {
	float:right;
}
.clear {
	clear:both;
}
.red {
	color:#FF0000;
}
.input {
	border:1px solid #ccc;
}
* {
	margin:0;
	padding:0;
}
#container {
	width: 872px;
	margin: 0 auto;
}
#head .banner {
	background:url(../images/02.gif) no-repeat top;
	height:82px;
	color: #FFFFFF;
}
#head .banner a {
	color:#FFFFFF;
}
#head .banner a:Hover {
	color:#A7C8E5;
}
#head .banner p {
	text-align:right;
	padding-right:120px;
	padding-top: 5px;
}
#head .nav {
	background:url(../images/03.gif) no-repeat;
	height:42px;
	line-height:42px;
	padding: 0 10px;
	color:#FFFFFF;
}
#head .nav a {
	color:#FFFFFF;
}
#head .nav a:Hover {
	color:#A7C8E5;
}
.content {
	background:url(../images/04.gif) repeat-x top #fff;
}
.content h3 {
	background:url(../images/07.gif) no-repeat;
	width:850px;
	margin:0 auto;
	height: 78px;
}
.content .ul_list {
	width:759px;
	height:721px;
	margin:0 auto;
	background:url(../images/14.gif) no-repeat;
	padding: 5px;
}
.content .ul_list li {
	width:33%;
	float:left;
	text-align:center;
	height: 200px;
	margin-bottom: 10px;
	background:url(../images/09.gif) no-repeat right bottom;
}
.content .ul_list li span {
	display:block;
	line-height:20px;
}
#foot {
	background:url(../images/10.gif) no-repeat;
	height:45px;
	padding-top: 10px;
}
#foot p {
	line-height:20px;
	text-indent:0;
	text-align:center;
	color:#FFFFFF;
}
/*userInfoModify.html*/
.userinfo {
	width:759px;
	height:448px;
	background:url(../images/11.gif) no-repeat;
	margin: 0 auto;
	padding-top:80px;
}
.tuihuo {
	width:759px;
	height:448px;
	margin: 0 auto;
	padding-top:80px;
}
.userinfo1 {
	width:759px;
	height:448px;
	margin: 0 auto;
	padding-top:80px;
}
.userinfo ul {
	width:70%;
	margin:0 auto;
}
.userinfo li {
	line-height:25px;
	height: 25px;
}
.userinfo li span {
	float:left;
	width: 80px;
}
.userinfo li input {
	border:1px solid #ccc;
}
.userinfo .button_img {
	margin:20px;
	text-align:center;
}
/*cartDetails.html*/
.cartDet {
	background:url(../images/14.gif) no-repeat;
	width:759px;
	height:721px;
	margin:0 auto;
	padding-top:15px;
}
.cartDet h4 {
	background:url(../images/15.gif) no-repeat;
	font-size:14px;
	line-height:32px;
	height:32px;
	padding-left:15px;
	margin:0 10px 10px;
}
.cartDet .bg {
	background:url(../images/16.gif) no-repeat right bottom;
	height: 630px;
	margin: 0 10px 10px;
}
.cartDet table {
	width:90%;
	margin:10px auto;
	text-align:center
}
.cartDet table, td, th {
	border:1px solid #ccc;
	border-collapse:collapse;
}
.cartDet th {
	line-height:30px;
}
.cartDet td {
	line-height:25px;
}
.cartDet .order_bg {
	background:url(../images/17.gif) no-repeat right bottom;
	height: 630px;
	margin: 0 10px 10px;
}
/*login.html*/
.login_bg {
	background:url(../images/18.gif) no-repeat;
	width:446px;
	height:251px;
	margin:118px auto;
}
.login_bg p {
	text-indent: 0px;
	width: 95%;
	padding-top:180px;
	margin:0 auto;
}
.login_bg input {
	border:1px solid #ccc;
}
/*main.html*/
.main {
	background:#EEF2FB;
	width: 800px;
	margin: 0 auto;
}
.main .top {
	background:url(../images/21.gif) no-repeat top;
	height:90px;
	padding-left:260px;
}
.main .top p {
	float:left;
	margin-top: 60px;
	font-size: 14px;
	font-weight: bold;
	color:#1E50A2;
}
.main .top p a {
	padding:0 10px;
}
.main .top p a:Hover {
	color:#fff;
}
.main .adm_con {
	 background:#F7F8F9;
}
.main .adm_con .bg_1 {
	background:url(../images/28.gif) repeat-y right;
}
.main .adm_con .bg_2 {
	background:url(../images/29.jpg) repeat-x bottom;
	height:650px;
}
.main .adm_con .bg_3{
	background:url(../images/27.gif) repeat-y left;
}
.main .adm_con h3 {
	background:url(../images/24.gif) repeat-x;
	height:29px;
	font-size:12px
}
.main .adm_con h3 span {
	background:url(../images/26.jpg) no-repeat center;
	width:85px;
	height:29px;
	text-align:center;
	display: block;
	line-height: 29px;
}
.main .adm_con table {
	width:90%;
	margin:10px auto;
	text-align:center
}
.main .adm_con table, td, th {
	border:1px solid #ccc;
	border-collapse:collapse;
}
.main .adm_con th {
	line-height:30px;
}
.main .adm_con td {
	line-height:25px;
}
.main .adm_con .padd {
	margin:10px 20px;
}


/*add*/
.main .add{
	margin:10px auto;
	width: 70%;
}
.main .add li{
	line-height:30px;
	height:30px;}
.main .add li span{
	float:left;
	width: 80px;
}
